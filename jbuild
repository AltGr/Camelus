(jbuild_version 1)

(library(
  (name camelus_lib)
  (public_name camelus)
  (wrapped false)
  (modules camelus_lib)
  (libraries (lwt.unix conduit-lwt cohttp-lwt opam-format opam-solver opam-state cohttp-lwt-unix fpath yojson nocrypto github-unix))
  (preprocess (pps (lwt_ppx)))
))

(executable(
  (name camelus_main)
  (public_name camelus)
  (modules camelus_main)
  (libraries (camelus))
  (preprocess (pps (lwt_ppx)))
))

(executable(
  (name camelus_replay)
  (public_name camelus-replay)
  (package camelus)
  (modules camelus_replay)
  (libraries (camelus))
  (preprocess (pps (lwt_ppx)))
))
